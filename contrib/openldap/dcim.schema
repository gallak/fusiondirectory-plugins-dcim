# Object Class
objectclass (1.3.6.1.4.1.38414.131.2.1 NAME 'fdDcimDatacenter'
        DESC 'FusionDirectory - Dcim Datacenter config'
        MUST ( cn $ description )
        MAY ( fdDcimDatacenterManagerDn $ l $ st $ postalAddress $ fdDcimDatacenterGPS $ fdDcimPowerCapacity $ fdDcimCoolingCapacity))

# Object Class
objectclass (1.3.6.1.4.1.38414.131.2.2 NAME 'fdDcimZone'
        DESC 'FusionDirectory - Dcim Zoneconfig'
        MUST ( cn $ description )
        MAY ( fdDcimZoneAuthorizedPersonDn $ fdDcimZoneDatacenter $ fdDcimZoneCode ))

# Object Class
objectclass (1.3.6.1.4.1.38414.131.2.3 NAME 'fdDcimCabinet'
        DESC 'FusionDirectory - Dcim Cabinet config'
        MUST ( cn  )
        MAY ( fdDcimCabinetZone $ fdDcimCabinetSize $ description))

# Object Class
objectclass (1.3.6.1.4.1.38414.131.2.4 NAME 'fdDcimComponent'
        SUP top AUXILIARY
        DESC 'FusionDirectory - Dcim component config'
        MUST ( )
        MAY ( fdDcimComponentManagerDn $ fdDcimComponentCabinetDn $ fdDcimComponentPosition $ fdDcimComponentSize $ fdDcimComponentRearImage $ fdDcimComponentFrontImage $ fdDcimComponentZoneDn $ fdDcimComponentStartSequence $ fdDcimComponentStopSequence $ fdDcimVirtualManagementServer))
